{
    "workbench.colorTheme": "One Dark Pro",
    "oneDarkPro.bold": false,
    "editor.fontFamily": "Iosevka Carbo",
    "editor.fontSize": 18,
    "editor.fontLigatures": true,
    "C_Cpp.updateChannel": "Insiders",
    "editor.cursorSmoothCaretAnimation": true,
    "terminal.external.linuxExec": "/usr/local/bin/st",
    "window.zoomLevel": 1,
    "terminal.integrated.fontFamily": "Iosevka Carbo",
    "terminal.integrated.fontSize": 18,
    "workbench.iconTheme": "vscode-icons",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "window.titleBarStyle": "custom",
    "bracketPairColorizer.showBracketsInGutter": true,
    "bracketPairColorizer.showHorizontalScopeLine": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "punctuation.definition.comment",
                    "comment.line.double-slash",
                    "variable.parameter",
                    "keyword.control.import.python",
                    "keyword.control.flow.python",
                    "entity.name.function.python",
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "workbench.colorCustomizations": {
        "activityBar.activeBorder": "#98c379",
        "activityBar.foreground": "#98c379",
        "activityBar.inactiveForeground": "#6e7179",
        "terminal.ansiBlack": "#4B5058",
        "terminal.ansiBrightBlack": "#4B5058",
        "terminal.ansiRed": "#E56464",
        "terminal.ansiBrightRed": "#E56464",
        "terminal.ansiBlue": "#61AFEF",
        "terminal.ansiBrightBlue": "#61AFEF",
        "terminal.ansiGreen": "#98C379",
        "terminal.ansiBrightGreen": "#98C379",
        "terminal.ansiYellow": "#F5D07B",
        "terminal.ansiBrightYellow": "#f5D07B",
        "terminal.ansiMagenta": "#C678DD",
        "terminal.ansiBrightMagenta": "#C678DD",
        "terminal.ansiCyan": "#35CACF",
        "terminal.ansiBrightCyan": "#35CACF",
        "terminal.ansiWhite": "#EDEDED",
        "terminal.ansiBrightWhite": "#EDEDED",
    },
    "editor.renderWhitespace": "all",
    "C_Cpp.dimInactiveRegions": false,
    "git-graph.graphColours": [
        "#61afef",
        "#c678dd",
        "#98c379",
        "#f5d07b",
        "#e56464",
        "#35cacf",
        "#ededed",
    ],
    "files.autoSave": "off"
}
